    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "eslint": "5.16.0",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "mini-css-extract-plugin": "0.6.0",
    "node-sass": "4.11.0",
    "postcss-cli": "6.1.2",
