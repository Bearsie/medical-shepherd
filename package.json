{
  "name": "medical-shepherd",
  "version": "1.0.0",
  "description": "An adroid mobile application that allows diagnosing diseases based on given symptoms and recommends going to the right doctor, enables to store prescriptions and information about medical appointments.",
  "license": "MIT",
  "keywords": [
    "framework7",
    "react",
    "ionic",
    "webpack",
    "cordova"
  ],
  "author": "Wojciech Niedźwiedź, Kamil Smoleń, Grzegorz Roczniak, Patryk Maziarz",
  "private": true,
  "main": "./src/main.jsx",
  "scripts": {
    "start": "yarn run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "emulate": "yarn run build && cordova run android"
  },
